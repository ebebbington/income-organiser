{
  "name": "sample-electron-app",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "npm i",
    "package-mac": "node_modules/.bin/electron-packager . 'Income Organiser' --overwrite --platform=darwin --out ./releases",
    "package-win": "node_modules/.bin/electron-packager . 'Income Organiser' --overwrite --platform=win32 --out ./releases",
    "package-linux": "node_modules/.bin/electron-packager . 'Income Organiser' --overwrite --platform=linux --out ./releases"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^8.0.0"
  },
  "dependencies": {
    "electron-packager": "^14.2.1",
    "electron-reload": "^1.5.0"
  }
}
